extends ../../layout

block content
  .row
    .col-md-12
      .panel.panel-default
        .panel-heading
          h4= title
        .panel-body
          table.table.table-striped
            thead
              tr
                th
                th Student Number
                th First Name
                th Last Name
                th Major
                th Year
                th Semester
                th Term
                th Level
                th Section
                th Interview Number
                th Interview Score
            tbody
              if results
                each row in results
                  tr
                    td
                      a.btn.btn-primary.btn-xs(href='/webforms/interviews/#{row.Semester_id}/') Click
                    td= row.Student_number
                    td= row.First_name
                    td= row.Last_name
                    td= row.Major
                    td= row.Year
                    td= row.Season
                    td= row.Term
                    td= row.Level
                    td= row.Section
                    td= row.Count || 0
                    td= row.Score || 0
              else
                tr
                  td Empty

          